package prob011055;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] A = new int[N];
		for (int i = 0; i < N; i++) {
			A[i] = sc.nextInt();
		}
		
		int[] sum = new int[N];
		sum[0] = A[0];
		for (int i=0; i<N-1; i++) {
			
			if(A[i]<=A[i+1]) {
				sum[i+1]
			}
		}
	}
}
